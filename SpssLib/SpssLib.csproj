<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>SpssLib</PackageId>
    <Company>TNS</Company>
    <Version>1.2.0</Version>
    <Description>A library for reading and writing .sav files</Description>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/TNSGlobal/SPSS-.NET-Reader</PackageProjectUrl>
    <PackageIconUrl>http://www.tnsglobal.com/sites/default/files/tns-logo.jpg</PackageIconUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/lnu/SPSS-.NET-Reader/master/LICENSE</PackageLicenseUrl>
    <PackageTags>SPSS PSPP SAV</PackageTags>
    <Authors>Roeland Nieuwenhuis, Francisco Biagi</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/lnu/SPSS-.NET-Reader</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>https://github.com/TNSGlobal/SPSS-.NET-Reader/releases</PackageReleaseNotes>
    <AssemblyTitle>SpssLib</AssemblyTitle>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.5.1" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
